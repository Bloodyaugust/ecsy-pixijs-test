{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "src/index.js",
  "author": "Bloodyaugust <greysonrichey@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "ecsy": "^0.4.2",
    "pixi.js": "^5.3.3",
    "stats.js": "^0.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "electron": "^10.1.3",
    "electron-builder": "^22.9.1",
    "file-loader": "^6.1.1",
    "mini-css-extract-plugin": "^1.0.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.1.2",
    "prettier-config-standard": "^1.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.0",
    "webpack": "^5.1.2",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "build": "webpack",
    "electron": "electron ./dist-electron/main.js",
    "electron-build": "rm -rf builds/ && electron-builder",
    "prettier": "prettier --write .",
    "start": "webpack serve"
  },
  "build": {
    "appId": "com.bloodyaugust.ecsy-pixijs-test",
    "productName": "ECSY PixiJS Test",
    "files": ["package.json", {"from": "./dist-electron/", "to": "./"}, {"from": "./dist", "to": "../dist"}],
    "win": {
      "target": "portable"
    },
    "directories": {
      "output": "builds"
    },
    "extraMetadata": {
      "main": "./main.js"
    },
    "asar": false
  }
}
